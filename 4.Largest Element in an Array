#include<stdio.h>
int main()
{
	int i,n,max,arr[n];
	printf("enter the number of elements in an array:");
	scanf("%d",&n);
	printf("enter %d elements:\n",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	max=arr[0];
	for(i=1;i<n;i++)
	{
		if(arr[i]>max)
		{
			max=arr[i];
		}
	}
	printf("The largest element in an array:%d\n",max);
	return 0;
}
